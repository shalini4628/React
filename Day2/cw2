import React from 'react';

function Cw()
{
    return(
        <div style={{backgroundColor:'lightblue',color:'darkblue' ,padding:'10px',bordercolor:'1px solid blue',border:'1px solid blue',borderRadius:'5x'}}>
            <h1 style={{color:"green"}}>Inline Style in JSX Example</h1>
            <p style={{color:"darkblue",fontsize:'16px'}}>This is a paragraph with inline styles applied.</p>
        </div>
    )
}
export default Cw
